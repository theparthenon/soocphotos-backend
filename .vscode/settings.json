{
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
      },
      "isort.args": [
        "--profile",
        "black"
      ],
      "python.analysis.autoImportCompletions": true,
      "pylint.args": [ "--load-plugins=pylint_django", "--django-settings-module=core.settings" ]
}